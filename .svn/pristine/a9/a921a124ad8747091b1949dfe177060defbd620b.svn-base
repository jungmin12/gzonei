.join_wrap .ui_radio--custom input[type=radio]{display:none;}
.join_wrap .ui_radio--custom input[type=radio][type="radio"]:checked + label{border-color:#333333}
.join_wrap .ui_radio--custom input[type=radio][type="radio"]:checked + label:before{background:url(../css/gzonei/SRC/images/program/mypage_bg2.png) no-repeat;background-position:center;}
