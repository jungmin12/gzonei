/* btn */
.btn {display:inline-block;min-width:90px;height:50px;padding:10px 10px 12px;font-size:16px;font-weight:500;border:1px solid #666;color:#666;border-radius:4px;text-align:center;line-height:1.7;font-size:16px;vertical-align:middle;transition: all .5s;}
.btn-default,.btn-default:link,.btn-default:visited {border-color:#aeabab;background-color:#fff;color:#333}
.btn-default.active,.btn-default:active,.btn-default:focus,.btn-default:hover,.open .dropdown-toggle.btn-default {border-color: #aeabab;background-color: #fff;color: #333}
.btn-secondary,.btn-secondary:link,.btn-secondary:visited {border-color: #ddd;background-color:#ededed;color:#333;}
.btn-secondary.active,.btn-secondary:active,.btn-secondary:focus,.btn-secondary:hover,.open .dropdown-toggle.btn-secondary {border-color: #ededed;background-color: #ededed;color: #333}
.btn-primary,.btn-primary:link,.btn-primary:visited {border-color:#fbcb00;background-color:#fbcb00;color:#333}
.btn-primary.active,.btn-primary:active,.btn-primary:focus,.btn-primary:hover,.open .dropdown-toggle.btn-primary {border-color: #fbcb00;background-color: #fbcb00;color: #333}
.btn-success,.btn-success:link,.btn-success:visited {border-color:#aeabab;background-color:#fff;color:#333}
.btn-success.active,.btn-success:active,.btn-success:focus,.btn-success:hover,.open .dropdown-toggle.btn-success {border-color: #aeabab;background-color: #fff;color: #333}


/* modal */
.modal-open #top_layout{z-index:-1}
.modal:before{content:'';display:inline-block;height:100%;width:0;margin-left:-1em;vertical-align:middle}
.modal__open{overflow:hidden;height:100%}
.modal__open .modal{overflow-x:hidden;overflow-y:auto;z-index:99999}
.modal__backdrop{position:absolute;top:0;right:0;left:0;background-color:rgba(0,0,0,.6)}
.modal__body{position:fixed;top:50%;left:50%;display:inline-block;width: 360px;margin:-1% auto 0;background:#fff;box-shadow:0 0 24px rgba(0,0,0,.5);transform:translate(-50%,-50%);transition:all .4s ease-out;vertical-align:middle;text-align:left;padding: 10px 0 0 0;border-radius: 5px;overflow: hidden;}
.modal__header{margin:0 15px;padding:10px 0;border-bottom:1px solid #e6e6e6}
.modal .modalTitle,.modal__title{font-size:1.2em;color:#000}
.modal__content{position:relative;padding:20px;overflow-y:auto}
.modal__footer{padding:8px 15px;border-top:1px solid #e6e6e6;background:#fafafa;text-align:center}
.modal__footer .btn{margin:0 1px;height: auto;padding: 7px 10px 7px;}
.modal__close{position:absolute;top:0;right:0;font-size:0}
.modal__close-button span{display:inline-block;width:73px;height:73px;background:#000}
.modal__close-button span:after,.modal__close-button span:before{display:inline-block;position:absolute;top:50%;left:50%;background-color:#fff;vertical-align:middle;transform:rotate(-45deg);content:'';}
.modal__close-button span:before{left:50%;width:48px;height:1px;margin-top:0;margin-left:-24px}
.modal__close-button span:after{width:1px;height:48px;margin-top:-24px}
.modal.fade .modal__backdrop{opacity:0;transition:opacity 1s}
.modal.fade.active .modal__backdrop{opacity:.75;transition:opacity 1s}
@media only screen and (max-width:770px){
    .modal__close-button span{width:50px;height:50px}
    .modal__close-button span:before{left:50%;width:28px;height:1px;margin-top:0;margin-left:-14px}
    .modal__close-button span:after{width:1px;height:28px;margin-top:-14px}
}



