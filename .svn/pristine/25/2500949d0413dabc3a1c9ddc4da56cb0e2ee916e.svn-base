.design-select .selected:after {content:attr(data-title); font-size: 15px; margin-left: 10px;}
.design-select .arrow {position: absolute;top: 50%;right: 5px;display: block;width: 11px;height: 6px;margin-top: -3px;background-repeat: no-repeat;background: transparent url(../images/program/arrow-select.png) no-repeat 88% 50%;background: transparent url(../images/program/arrow-select.png) no-repeat 88% 50%;}
.design-select.select-focus .selected {color: #838b93;}
