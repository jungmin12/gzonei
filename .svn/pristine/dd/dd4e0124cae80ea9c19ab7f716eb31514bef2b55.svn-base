/* 투자 정보 검색창 css */
.searchResult{display:none;position:absolute;top: 234px;left:0;background-color: #fff;width: 100%;height: 100%;}
.searchResult .searchResult__index{padding:28px 0 20px;text-align:center}
.searchResult .searchResult__index strong{color:#000;font-size:16px;}
.searchResult .searchResult__index strong *{vertical-align:middle}
.searchResult .searchResult__index strong em{display:inline-block;font-size:17px;color:#000;font-weight:bold;vertical-align:top}
.searchResult .small-select .selected{font-size:14px;font-weight: bold;color: #000;}
.searchResult  .searchResult__obj{height: 861px;}
.searchResult  .searchResult__obj .item{display:block;}
.searchResult  .searchResult__obj .item .link{position: relative;display:block;padding:39px 27px;padding-right:68px;border-top:1px solid #eee;text-align: left;width: 100%;}
.searchResult  .searchResult__obj .item .link.on,
.searchResult  .searchResult__obj .item .link:hover{background-color:#f6f6f6}
.searchResult  .searchResult__obj .item .link strong,
.searchResult  .searchResult__obj .item .link span{display:block;}
.searchResult  .searchResult__obj .item .link strong{color:#222;font-size:16px;font-weight:bold;line-height:1.3;margin-bottom: 6px;}
.searchResult  .searchResult__obj .item .link strong em{display:block;font-size:14px;font-weight:normal}
.searchResult  .searchResult__obj .item .link span{color:#444;font-size:14px;line-height:1.3}
.searchResult  .searchResult__obj .item .link i{display:inline-block;padding-right:23px;font-weight:bold;color:#000;font-size:14px;font-style: normal;margin-top: 8px;line-height: 20px;}
.searchResult  .searchResult__obj .mCSB_inside > .mCSB_container{margin-right:0;}
.searchResult .searchResult__index .close{position:absolute;top:0;right:0;opacity:1;width:30px;height:30px;font-size:0;background: url(../../images/program/icon-close.png) no-repeat;}

