#searchResult .searchResult__obj .item .link:after{content:'';position:absolute;top:50%;right:20px;transform:translate(0,-50%);width:45px;height:45px;background:url(../../images/program/icon-map.png) no-repeat;}
#searchResult .searchResult__obj .item .link i{display:none;background: url(../../images/program/icon-arrow.png) no-repeat 98% 50%;transition: all .3s;opacity: 0;}
#searchResult .searchResult__obj .item .link.on i,
#searchResult .searchResult__obj .item .link:hover i{display:inline-block;opacity: 1;}

.map__header{position: relative;display: block;padding:12px 67px 10px 23px;line-height: 1;border: 1px solid #333;border-radius: 5px;background: url(../../images/program/icon-open.png) no-repeat 95% 50%;}
