.button-map{position:absolute;top:40px;left:62px;}
.button-map #infoAdd,
.button-map #infoParagraph{display:block;font-size:15px;padding-left:22px;margin-bottom:10px;color:#565656;font-size:15px;}
.button-map #infoAdd{background:url(../../images/program/icon-1.png) no-repeat;}
.button-map #infoParagraph{background: url(../../images/program/icon-2.png) no-repeat 0 50%;}
.btn *{vertical-align:middle}
.icon-file,
.icon-print{display:inline-block;}

.icon-file{margin: 0 6px 0 5px;width:14px;height:14px;background:url(../../images/program/icon-file.png) no-repeat;}
.icon-print{width:14px;height:14px;background:url(../../images/program/icon-print.png) no-repeat;}

.photoArea{position:relative;padding-left:242px;min-height:156px;margin-bottom:50px}
.photoArea .thumbnail{position:absolute;top:0;left:0;width:207px;height:156px;background-color:#efefef;padding: 0;}
.photoArea .thumbnail a{display:block;width:100%;height:100%;}
