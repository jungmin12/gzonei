.photoList{}
.photoList ul li{display:block;position:relative;padding-left:110px;color:#666;font-size:15px;margin:3px 0;line-height:1.3}
.photoList ul li strong{position:absolute;top:0;left:0;max-width:90px;color:#333;font-size:15px;font-weight:bold}


#iconSort{border-radius:30px;padding: 12px 33px;position:absolute;top:40px;left:50%;transform:translate(-50%,0);background-color: #ffffff;box-shadow: 0px 5px 5px rgb(0 0 0 / 16%);}
