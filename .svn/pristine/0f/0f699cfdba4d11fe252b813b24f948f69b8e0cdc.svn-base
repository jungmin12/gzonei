/* hidden type */
.hide,
.skip,
.sr-only{position:fixed;top:0;left:0;width:0;height:0;font-size:0;overflow:hidden}
 
#totalWrap{position:relative;padding-left:390px;}

#body_layout.map{}
#body_layout.map *{font-family: 'gothic'}
#body_layout.map .body_wrap{max-width:none;}
