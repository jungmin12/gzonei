#info{position:absolute;z-index: 11;transition:0.25s ease-in-out;top:0;left:390px;width:768px;height:100%;background:#fff;}


#mapTabs{position:absolute;top:34px;left:410px;z-index: 9;}
#mapTabs button{display:inline-block;width:57px;height:57px;background-color:#fff;border-radius:5px;font-size: 12px;}
#mapTabs button.on{border:2px solid #333333;}
#mapTabs button:before{width:29px;height:27px;}
#mapTabs button.on.naver:before{background-position:-281px -9px}
#mapTabs button.naver:before{background-position:-281px -38px}
#mapTabs button.on.kakao:before{background-position: -333px -8px;}
#mapTabs button.kakao:before{background-position: -333px -37px;}
#mapTabs button.on.google:before{background-position: -381px -7px;}
#mapTabs button.google:before{background-position: -381px -36px;}



#searchForm form{position:relative;background-color:#fff;border-radius:5px;}
#searchForm form #searchKeyword{height:44px;line-height:44px;width:100%;}
#searchForm form #searchSubmit{position:absolute;top: 10px;right: 10px;width:20px;height:21px;font-size: 0;}
#searchForm form #searchSubmit:before{background-position:-4px -34px}
#searchForm form .mapLocationApi{position:absolute;top: -26px;right: 0;padding-left:20px;font-size: 14px;letter-spacing: -1px;font-weight: bold;color: #ffdf00;line-height: 1.2;}
#searchForm form .mapLocationApi:before{position:absolute;top:1px;left:0;width:15px;height:15px;background-position:-55px -7px}

#keyword{position:relative;padding-left:90px;margin-top: 15px;}
#keyword em{font-size:15px;color:#fff;font-weight:bold;position:absolute;top:0;left:0;width:90px;letter-spacing: -1px;}
#keyword .grap{position:relative;width:100%;}
#keyword .grap a{display:inline-block;color:#a5a5a5;font-size: 14px;margin: 0 2px 0 0;}


#searchFormTab .tab-ul{}
#searchFormTab .tab-ul:after {display:block;visibility:hidden;clear:both;content:""}
#searchFormTab .tab-ul li{display:inline-block;float: none;width:auto;height: auto;border:0;background: none;width: 50%;float: left;text-align: center;}
#searchFormTab .tab-ul li a{display:block;position: relative;padding: 22px 0 20px;line-height: 1.2;text-align: center;font-size: 19px;background-color: #4a4a4a;color:#fff;font-weight:bold}
#searchFormTab .tab-ul li.on a{background-color:#fff;color:#4a4a4a}
#searchFormTab .obj{display:none;position: relative;/*min-height:620px;*/padding:44px 0;padding-left: 37px;padding-right: 10px;}
#searchFormTab .obj.on{display:block;}
#searchFormTab .obj .scrollbar{height:580px;padding-right:30px;}



#searchFormTab .obj .form-group{margin-left:-3px;margin-bottom:10px;}
#searchFormTab .obj .scrollbar{padding-right:30px;}
#searchFormTab .obj .form-control{border-radius:0;height:40px;line-height:40px;}
#searchFormTab .obj .form-group .col-sm-4{padding:0 3px;}
#searchFormTab .obj .form-group .col-sm-6{padding:0 3px;}
#searchFormTab .obj .form-group .col-sm-12{padding:0 3px}
#searchFormTab .obj .form-group .control-label{display:block;padding-left:5px;}
#searchFormTab .obj .form-group .control-label label{padding:0;margin:0;}




#searchFormTab .obj .form-group:after {display:block;visibility:hidden;clear:both;content:""}
#searchFormTab .obj .search--btn{display:block;clear: both;padding-top: 0;}
#searchFormTab .obj .search--btn .btn--submit{width:calc(100% - 4px);display: block;height: 50px;/* display: block; */}
#searchFormTab .obj .search--btn .btn--submit .submit{padding: 10px 10px 12px;padding-left: 35px;font-size:16px;height: 100%;border-radius: 5px;color: #333;background:#fbcb00 url(../../images/program/icon-search.png) no-repeat 0 50%;background-position: 28px 50%;font-weight: bold;display: block;width: 100%;}

#searchFormTab .obj#tab-panel2 .form-group .col-sm-6 span{display:block;font-weight:bold;margin:5px 0;font-size: 13px;}

#searchResult{display:none;position:absolute;top: 234px;left:0;background-color: #fff;width: 100%;height: 100%;}
#searchResult .searchResult__index{padding:28px 0 20px;text-align:center}
#searchResult .searchResult__index strong{color:#000;font-size:16px;}
#searchResult .searchResult__index strong *{vertical-align:middle}
#searchResult .searchResult__index strong em{display:inline-block;font-size:17px;color:#000;font-weight:bold;vertical-align:top}
#searchResult .small-select .selected{font-size:14px;font-weight: bold;color: #000;}
#searchResult  .searchResult__obj{height: 861px;}
#searchResult  .searchResult__obj .item{display:block;}
#searchResult  .searchResult__obj .item .link{position: relative;display:block;padding:39px 27px;padding-right:68px;border-top:1px solid #eee;text-align: left;width: 100%;}
#searchResult  .searchResult__obj .item .link.on,
#searchResult  .searchResult__obj .item .link:hover{background-color:#f6f6f6}
#searchResult  .searchResult__obj .item .link strong,
#searchResult  .searchResult__obj .item .link span{display:block;}
#searchResult  .searchResult__obj .item .link strong{color:#222;font-size:16px;font-weight:bold;line-height:1.3;margin-bottom: 6px;}
#searchResult  .searchResult__obj .item .link strong em{display:block;font-size:14px;font-weight:normal}
#searchResult  .searchResult__obj .item .link span{color:#444;font-size:14px;line-height:1.3}
#searchResult  .searchResult__obj .item .link i{display:inline-block;padding-right:23px;font-weight:bold;color:#000;font-size:14px;font-style: normal;margin-top: 8px;line-height: 20px;}
#searchResult  .searchResult__obj .mCSB_inside > .mCSB_container{margin-right:0;}
#searchResult .searchResult__index .close{position:absolute;top:0;right:0;opacity:1;width:30px;height:30px;font-size:0;background: url(../images/program/icon-close.png) no-repeat;}


#notice{position:absolute;bottom:0;left:0;width:100%;padding:30px 39px;border-top:1px solid #ddd;min-height:262px}
#notice strong{display:block;margin-bottom:14px;color:#444;font-size:16px;font-weight:bold}

#footerLink{position:relative;background-color:#f5f5f5}
#footerLink .layout{padding:28px 0;text-align:center}
#footerLink .layout .item{display:inline-block;}
#footerLink .layout .item a{display:block;padding:0 16px;color:#333;font-size:18px;position: relative;}
#footerLink .layout .item a:before{content:'';position:absolute;top:50%;left:0;width:1px;height:17px;background-color:#e0e0e0;transform: translate(0, -50%);}
#footerLink .layout .item:first-child a:before{display:none;}



.select-list span{font-size: 14px;font-weight: bold;color: #000;}
#totalWrap{border-top: 1px solid #ddd;}
#side{border-left: 1px solid #ddd;box-shadow:0px 15px 20px rgb(0 0 0 / 16%)}
#info{padding:125px 10px 60px 67px;box-shadow:0px 15px 20px rgb(0 0 0 / 16%);z-index: 10;}
#info .scroll{padding-right: 27px;height: 1040px;}
#info .title{display:block;margin-bottom:30px;color:#333;font-size:34px;line-height:1.3;font-weight:bold}
