.txt-tip{position: relative;padding-left:27px;color:#666;font-size:15px;}
.txt-tip:before{content:"";position:absolute;top:13px;left:0;width:16px;height:16px;background:url(../../images/program/icon-info.png) no-repeat;}
.menteu{margin:0 -20px;border:1px solid #414141;border-radius:5px;position: relative;padding:32px 30px 32px 169px}
.menteu:before{content:'';position:absolute;top:32px;left:32px;width:70px;height:63px;background:url(../../images/program/icon-title.png) no-repeat;}	

#info_close{position:absolute;top:40px;right:40px;font-size:16px;display:inline-block;padding-right:44px}
