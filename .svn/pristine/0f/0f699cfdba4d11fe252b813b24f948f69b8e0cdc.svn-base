.table-scroll .table tbody>tr>td, .table tbody>tr>th, 
.table-scroll .table tfoot>tr>td, .table tfoot>tr>th, 
.table-scroll .table thead>tr>td, .table thead>tr>th,
.table-scroll .table tbody td{padding: 4px;font-size: 14px;}
.table-scroll{height:165px;overflow:hidden;overflow-y:auto;margin-top: -10px;}


.modal_form .form-upload{margin-bottom:5px;position: relative;padding-right: 100px;}
.modal_form .form-control.disabled{background-color:#eee;opacity:1;cursor: not-allowed}
.modal_form .file-form-group{position:relative;white-space:nowrap;display:table}
.modal_form .form-previewbox,
.modal_form .form-checkbox,
.modal_form .form-input__addon,
.modal_form .form-group__upload{position:relative;display:table-cell;vertical-align: middle;}
.modal_form .form-previewbox{width:100px;padding-right: 10px;}
.modal_form .form-previewbox img{width:100%;}
.modal_form .form-checkbox,
.modal_form .form-input__addon{width:1%;padding-right: 4px;}
.modal_form .form-group__upload{vertical-align: middle;padding-right: 10px;padding-bottom: 5px;}
.modal_form .form-checkbox{padding-left:10px;}
.modal_form .form-group__upload .form-control{height:34px;}
.modal_form .form-upload__label{display:inline-block;margin-right:0;font-size:0;text-indent:100%;vertical-align:middle}
.modal_form .form-upload__label.file{margin-top:-10px}
.modal_form .form-upload__file{display:inline-block;width:inherit;min-width:80%;padding-left:0;line-height:16px}
.modal_form .form-upload__file--hidden{position:absolute;width:1px;height:1px;min-width:inherit;margin:-1px;overflow:hidden;clip:rect(0,0,0,0);border:none}
.modal_form .form-upload__clear{display:none;position:absolute;right:8px;top:9px;font-size:0;line-height:0;color:transparent}
.modal_form .form-upload__clear.active{display:block}
.modal_form .form-upload.focus [data-button=upload]{outline:2px dotted #2b69fc}

.modal_form .file-button{position:absolute;top: 0;right: 0;}
.modal_form .file-button button{display:inline-block;padding: 7px;/* width:50px; */border: 1px solid #aeabab;background-color: #fff;color: #333;border-radius: 5px;font-size: 12px;}

#form-map .top_head{margin-top:20px}


@media screen and (max-width:1024px){
[data-ly-type="content"].modal.fade .modal-dialog, [data-ly-type="content"].modal.in .modal-dialog{top:0;left:0;transform: translateX(0%);padding: 0;}
[data-ly-target="modal-map1"] .modal__body,
[data-ly-target="modal-map2"] .modal__body{width: 96%;top:0;left:0;transform: translate(0%,0%);}
.modal_form .form-horizontal.bucket-form .form-group>div[class^=col-], .modal_form .form-horizontal.bucket-form .form-group>div[class^=alert-], .modal_form .form-horizontal.bucket-form .form-group>div[class^=control-]{padding-top:2px;padding-bottom:2px;}
.modal_form .form-horizontal{padding:5px;}
[data-ly-target="modal-map1"] .modal__content, [data-ly-target="modal-map2"] .modal__content{height:90vh;}

}

@media only screen and (max-width:767px){
.table-scroll{display:block;width:100%;height:auto;overflow:hidden;overflow-x:auto;}
.table-scroll .table{width:995px;}
}

