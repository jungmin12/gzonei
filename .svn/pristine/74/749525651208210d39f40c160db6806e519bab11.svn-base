.ly__open #top_layout,
.confirm__open #top_layout{z-index:-1}

.modal:before{content:'';display:inline-block;height:100%;width:0;margin-left:-1em;vertical-align:middle}
.modal__open{overflow:hidden;height:100%}
.modal__open .modal{overflow-x:hidden;overflow-y:auto;z-index:99999}

.ly__open{overflow:hidden;height:100%}
.ly__open .modal{overflow-x:hidden;overflow-y:auto;z-index:99999}
.modal__backdrop{position:absolute;top:0;right:0;left:0;background-color:rgba(0,0,0,.6)}
.modal__body{position:fixed;top: 50%;left: 50%;display:inline-block;width: 650px;margin:-1% auto 0;background:#fff;box-shadow:0 0 24px rgba(0,0,0,.5);transform: translate(-50%,-50%);transition:all .4s ease-out;vertical-align:middle;text-align:left;padding: 0;border-radius: 5px;overflow: hidden;}

[data-ly-target="modal-pay"] .modal__body{width: 400px;}
.modal__dialog .modal__body{width: 360px;}
.modal__dialog .modal__header {position:relative;padding:17px 15px 16px 20px;background-color: #fbcb00;color:#333;font-weight: bold;}
.modal__header {position:relative;padding:17px 15px 16px 20px;background-color:#333;color:#fff}
.modal__header .modal__title{display: flex;flex-wrap: wrap;justify-content: left;align-items: baseline;width: 100%;margin: 0;font-size: 20px;color: #fff;font-weight: bold;}
.modal .modalTitle,.modal__title{font-size:1.2em;color:#000}
.modal__dialog .modal__header .modal__title{display: flex;flex-wrap: wrap;justify-content: left;align-items: baseline;width: 100%;margin: 0;font-size: 20px;color: #333;font-weight: bold;}

.modal__content{position:relative;padding:20px;overflow-y:auto}
.modal__footer{padding:8px 15px;border-top:1px solid #e6e6e6;background:#fafafa;text-align:center}
.modal__footer .btn{margin:0 1px;height: auto;padding: 7px 10px 7px;}
.modal__dialog .modal__footer{padding:0;border-top:none;border-top: 1px solid #d8d8d8;}
.modal__dialog .modal__footer .btn{background: #f7f7f7;width: 50%;height: 45px;line-height: 45px;border: 0;/* border-top: 1px solid #d8d8d8; */margin: 0;padding: 0px;/* border-left: 1px solid #d8d8d8; */border-radius: 0;}
.modal__dialog .modal__footer .btn.btn-default{border-right:1px solid #d8d8d8}
.modal__close{position:absolute;top:0;right:0;font-size:0}
.modal__close-button span{display:inline-block;width:73px;height:73px;background:#000}
.modal__close-button span:after,.modal__close-button span:before{display:inline-block;position:absolute;top:50%;left:50%;background-color:#fff;vertical-align:middle;transform:rotate(-45deg);content:'';}
.modal__close-button span:before{left:50%;width:48px;height:1px;margin-top:0;margin-left:-24px}
.modal__close-button span:after{width:1px;height:48px;margin-top:-24px}
.modal__close-button .close {display:flex;position: absolute;top: 10px;width:36px;height:36px;padding:0;margin:0;cursor:pointer;font-size:0;transition:all .7s;-webkit-transition:all .7s;right: 16px;opacity: 1;}
.modal__close-button .close:before,
.modal__close-button .close:after {position:absolute;top:50%; left:50%; content:'';display:block;background:#aaa;height:1px;width:32px;}
.modal__close-button .close:before {-webkit-transform:translateX(-50%) rotate(45deg);transform:translateX(-50%) rotate(45deg)}
.modal__close-button .close:after {-webkit-transform:translateX(-50%) rotate(-45deg);transform:translateX(-50%) rotate(-45deg)}
.modal__close-button .close:hover {-webkit-transform:rotate(180deg); transform:rotate(180deg)}
.modal.fade .modal__backdrop{opacity:0;transition:opacity 1s}
.modal.fade.active .modal__backdrop{opacity:.75;transition:opacity 1s}

[data-ly-type="content"].modal.fade .modal-dialog ,
[data-ly-type="content"].modal.in .modal-dialog {position: absolute;top: 0;left: 0;/* transform:translate(-50%, -50%);  -webkit-transform: translate(-50%, -50%); -ms-transform: translate(-50%, -50%);transition:transform .3s ease-out;*//* margin:-25% 0 0 -25%; */max-width:none;padding:10px;width: 100%;transform: translate(0%,0%);right: 0;bottom: 0;}
[data-ly-type="content"] .modal__footer{background-color:transparent;border-top:none;padding-bottom: 30px;}
[data-ly-type="content"] .modal__footer .btn{border-radius:30px;}
[data-ly-type="content"] .txt{text-align:center;display: block;width: 100%;}
[data-ly-type="content"] .txt strong{font-weight:bold;color:#000}
[data-ly-type="content"] .form-control{border-radius:30px;}
.h_title_sub {display:inline-block;margin:0 0 15px;font-size: 16px;color:#333;font-weight:700;}
.top_head { margin-bottom:15px; display:flex; justify-content:space-between; align-items:center}
.ly_align_center {display:flex; justify-content:space-between; align-items:center}


@media only screen and (max-width:770px){
    .modal__close-button span{width:50px;height:50px}
