.search .search--text{display:inline-block;/* min-width:25%; */margin:0;padding:0;width: 78%;margin-left: 5px;position: relative;}
.search .search--text span{display:block;padding: 0 17px;/* border:1px solid #aaa; */background-color:#fff;border-radius: 5px;}
.search .search--text .element_inline{display:inline-block;}
.search .search--text input[type="text"]{width:100%;height:57px;margin:0;padding:0;border:none;background-color:#fff;line-height:57px;}
