.sub0901_wrap .point{color: #cf0000; margin-top: 20px;}
.tab-ul{margin-bottom:90px;overflow: hidden;margin-left: -10px;}
.tab-ul:after{display:block;}
.tab-ul li{display:table;float:left;height:100%;}
.tab-ul li *{margin:0;padding:0;line-height:0;}
.tab-ul li a{display:table-cell;border:1px solid transparent;background-image:none;color:inherit;text-align:left;cursor:pointer;transition:background-color 50ms ease-in-out;-webkit-transition:background-color 50ms ease-in-out;border: 1px solid #ddd;background-color: #fff;}
.tab-ul li.on{background-color: #fff;}
.tab-ul li a span{display:block;position:relative;line-height:120%;}
.tab-ul li {display: inline-block;position: relative;z-index: 0;float: left;height: 168px;margin: 0 0 0 10px;width: calc((100%/6) - 10px); box-sizing: border-box;}
.tab-ul li a{display:table;position:relative;width:100%;height:100%; padding: 40px;text-align:center;}
.tab-ul li a span{display:table-cell;position:relative;z-index:1;margin:0;color:#666;font-size: 18px;text-align: center;font-weight: bold;line-height:1.4;vertical-align:middle;display: block;}
.tab-ul li a:hover:after,.tab-ul.type3 li.on a:after{background-position:-45px -1px;}
.tab-ul li a:hover:before,.tab-ul.type3 li.on a:before{position:absolute;z-index:1;top:-1px;left:-1px;right:-1px;bottom:-1px;/content:"";}
.tab-ul li a:hover span,.tab-ul.type3 li.on a span{color: dfdfdf;}
.tab-ul li.on a{border:4px solid #333;margin-bottom: 100px; font-color: #000;}
.tab-ul li .ir {position: relative;top: auto;left: auto; margin: 0 auto;width: 50px;height: 50px; background: url(../images/contents/sub09/sub0901_ir.png);}
.tab-ul li .ir.icon1 {background-position: 0 0;}
.tab-ul li .ir.icon2 {background-position: -50px 0;}
.tab-ul li .ir.icon3 {background-position: -100px 0;}
.tab-ul li .ir.icon4 {background-position: -150px 0;}
.tab-ul li .ir.icon5 {background-position: -205px 0;}
.tab-ul li .ir.icon6 {background-position: -255px 0;}
.tab-ul li.on .ir.icon1 {background-position: 0 -50px;}
.tab-ul li.on .ir.icon2 {background-position: -50px -50px;}
.tab-ul li.on .ir.icon3 {background-position: -100px -50px;}
.tab-ul li.on .ir.icon4 {background-position: -150px -50px;}
.tab-ul li.on .ir.icon5 {background-position: -205px -50px;}
.tab-ul li.on .ir.icon6 {background-position: -255px -50px;}
.tabpanel {display:none;text-align:center;}
.tabpanel.on{display:block;}
.tabpanel .tit{font-size:20px;font-weight: bold;}
.tabpanel .tab_tit{font-size: 28px;color: #333;font-weight: 600;}
