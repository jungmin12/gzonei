.tabpanel .btn{padding:10px 30px; margin:30px 5px; }
.tabpanel .btn a{background-color: ##fbcb00; border-color: ##fbcb00; color: #333;}
.tabpanel .box{min-height: 800px;margin:30px 0px 50px 0px; padding:3%;color: #ddd; font-weight: 600;border:5px solid #eee;position: relative;}
/*.tabpanel .box .box_txt{font-size: 40px;}*/
