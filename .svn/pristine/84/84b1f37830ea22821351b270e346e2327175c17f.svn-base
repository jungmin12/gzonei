.table-list table tbody tr td .td__num {padding-top: 7px;}
.table-list table tbody tr td.subject a {font-size: 14px;}
.table-list table tbody tr td {font-size: 14px;}

#invInfoListPagination a {margin-right:2px;}
.pagination li > a, .pagination .page-item > .page-link, .pagination .page-item > span {min-width: 20px;width: 40px;height: 40px;line-height: 40px;}


/* 개발 정보  사이드 페이지 부분 */
#searchFormTab .tab-ul li a {padding: 17px 0 16px;}

@media screen and (min-width:1024px) {
    #side { width: 330px; border-left: 0; }
    #searchFormTab .obj {padding-top: 27px;padding-left: 37px;padding-right: 10px;}
    #searchFormTab .tab-ul li a {font-size: 15px; padding: 17px 0 16px;}
    .search-text {color: #ffffff;font-size: 20px;}
    .mapTitle {font-size: 12px;}
    .form-control {font-size: 11px;}
    .alarmTxt {font-size: 12px;}
    #searchFormTab .obj .search--btn .btn--submit .submit {font-size: 14px;}
    #searchFormTab .obj .search--btn .btn--submit .submit:before {width: 18px;height: 18px;}
    .mapSearch__area .form-group .col-sm-6 > span {font-size: 12px}
    #searchFormTab .obj .form-group .search--calendar .btn__calendar {right: 12px;}
    label {font-size: 12px;}
    #totalWrap {padding-left: 330px;}
    #mapTabs {left: 350px;}
    .mapAll {margin-top: 0}

/*  개발 정보 검색해 들어가 보여지는 부분  */
    #searchResult .searchResult__index .close {width:24px;height: 24px;background-size: contain;}
    #searchResult .searchResult__index {padding: 22px 0 16px;}
    #searchResult .searchResult__obj .item .link:after {top:45%;width: 36px;height: 36px;background-size: contain;}
    #searchResult .searchResult__obj .item .link strong em {font-size: 12px;}
    #searchResult .searchResult__obj .item .link strong {font-size: 14px;}
    #searchResult .searchResult__obj .item .link span {font-size: 12px;}
    #searchResult .searchResult__obj .item .link {padding: 31px 21px;} /* padding 으로 줄이면 글씨 넘쳐지는 부분 어떻게 할것인지/ 지도 아이콘 어떨게 둘지 물어볼것*/

/* 개발 정보 개발 정보 목록 사이드 부분 */
    #sideAdmin strong.headTitle {font-size: 27px;}
    #sideAdmin .inner .scroll > .title {font-size: 27px; margin-bottom: 24px;}
    /* .text-center button_align .btn {margin-bottom: 10px;} 투자 정보 리스트에 들어가면 보이는 버튼 부분 위아래 간격 맞출지 안맞출지 */


/*  투자정보 리스트 부분  */
    #sideAdmin {padding: 48px 15px 48px 53px;} /* max,min-width:1350px로 맞출때 투자정보 리스트 가로 부분 맞출때 안맞는 부분 물어볼것 */
    #invtSrchList > .title {margin-bottom: 24px;}
    .box-list.all__list .item .link {padding: 40px 301px 40px 135px;}
    .box-list.all__list .item .description .title > em {font-size: 22px;}
    .box-list.all__list .item .td__category {left: 37px;top: 35px;}
    .mapInvLink {top: 92px;left: 47px;width: 55px;height: 55px;}
    .devInvLink {top: 165px;left: 47px;width: 55px;height: 55px;}
    .SideOff {top:30px;right:50px;height: 24px;background: url(../css/gzonei/SRC/images/program/icon-close.png) no-repeat 50% 50%;}
    .box-list.all__list .item .description ul > li {white-space: nowrap;text-overflow: ellipsis;overflow: hidden;} /*투자정보 리스트 text 말줄임...할지안할지*/
    #invInfoListPagination a {margin-right: 2px;}
    .pagination li > a, .pagination .page-item > .page-link, .pagination .page-item > span {min-width: 20px;width: 40px;height: 40px;line-height: 40px;}
