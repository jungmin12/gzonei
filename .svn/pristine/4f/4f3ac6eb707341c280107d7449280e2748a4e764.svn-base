	<!-- 회원수 조회 -->
	<select id="selectDevlopInfoListTotCnt" parameterType="EgovMap" resultType="int">
		<![CDATA[
			SELECT COUNT(*)
			FROM	(SELECT * 
						FROM PUBLIC.GZ_DEVLOP_INFO 
					WHERE 
						DWK_OTHBC_AT = 'Y'
