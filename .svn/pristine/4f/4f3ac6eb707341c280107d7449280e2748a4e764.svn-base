	<select id="selectDevlopInfoDetail" parameterType="EgovMap" resultType="EgovMap">
		<![CDATA[
			SELECT A.*, B.UNIT_TY, ST_ASTEXT(ST_TRANSFORM(A.GEOM, 3857)) AS wkt
			FROM GZ_DEVLOP_INFO A, GZ_BSNS_DSTRC_CODE B
