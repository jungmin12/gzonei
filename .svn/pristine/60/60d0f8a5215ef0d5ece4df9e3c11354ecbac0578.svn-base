#mvisual #popupzone{position: absolute;left: 0;bottom: 0;z-index: 99;width: 328px;-webkit-animation: slide-in-top 2s cubic-bezier(.25,.46,.45,.94) both;animation: slide-in-top 2s cubic-bezier(.25,.46,.45,.94) both;}
body.login #mvisual #popupzone,
body.open #mvisual #popupzone{bottom:0;}
#mvisual #popupzone .grap{overflow:hidden;width: 100%;height: 211px;}
#mvisual #popupzone .grap .obj{position:relative;width: 100%;height: 100%;}
#mvisual #popupzone .grap .obj li{position:absolute;top:0;left:0;width: 100%;height: 100%;}
#mvisual #popupzone .grap .obj li a{display:block;height: 100%;text-align: center;}
#mvisual #popupzone .grap .obj li img{height:100%}
#mvisual #popupzone .controlBox{position:absolute;left: 0;top: -34px;width: 100%;text-align: right;}
#mvisual #popupzone .controlBox > strong{display:inline-block;margin-right: 10px;font-size:16px;color:#fff;font-family:'score';line-height: 44px;vertical-align: middle;font-weight: bold;}
#mvisual #popupzone .controlBox > button{display: inline-block;position:relative;width:7px;height: 18px;margin-left: 12px;font-size:0;}
#mvisual #popupzone .controlBox > .btn_next:before,
#mvisual #popupzone .controlBox > .btn_prev:before{content:'';position:absolute;top: 50%;left: 1px;display:block;width: 9px;height: 9px;margin-top: -6px;border: 1px solid #fff;border-width: 2px 2px 0 0;}
#mvisual #popupzone .controlBox > .btn_next:before{left: -3px;transform: rotate(45deg);-webkit-transform: rotate(45deg);}
#mvisual #popupzone .controlBox > .btn_prev:before{transform: rotate(-135deg);-webkit-transform: rotate(-135deg);}
#mvisual #popupzone .controlBox > .btn_stop,
#mvisual #popupzone .controlBox > .btn_play{width: 18px;border:1px solid #fff;}
#mvisual #popupzone .controlBox > .btn_stop:before{content:'';position:absolute;top: 50%;left: 7px;right: 0;display:block;width: 3px;height: 8px;margin-top: -3.5px;box-shadow: -1px 0 #fff, 1px 0 #fff;}
#mvisual #popupzone .controlBox > .btn_play:before{content:'';position:absolute;top: 50%;left: 5px;right:20px;display:block;width: 0;height: 0;margin-top: -3.5px;border-width: 4px 0 4px 6px;border-style: solid;border-color: transparent transparent transparent #fff;}
#mvisual #popupzone .controlBox .number{position: absolute;left: 10px;top: 0;color: rgba(255,255,255,0.5);font-size: 16px;letter-spacing:3px;font-family: 'score';}
#mvisual #popupzone .controlBox .number button{width:9px;height:9px;background:#8f8f8f;border-radius:50%;font-size:0;}
#mvisual #popupzone .controlBox .number .ov{background:#fff;}

@-webkit-keyframes slide-in-top{0%{-webkit-transform:translateY(-1000px);transform:translateY(-1000px);opacity:0}100%{-webkit-transform:translateY(0);transform:translateY(0);opacity:1}}@keyframes slide-in-top{0%{-webkit-transform:translateY(-1000px);transform:translateY(-1000px);opacity:0}100%{-webkit-transform:translateY(0);transform:translateY(0);opacity:1}}

