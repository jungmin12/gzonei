@charset "utf-8";


#op_layout{display: block;width:100%;background: #242b38;text-align:center;padding:0;margin:0;position:relative;z-index:999;}
#op_layout.on{display:block;}
#op_layout #op_slide{display: none;height: 200px;padding: 0;}
#op_layout.on #op_slide{display: block;}
#op_layout #op_slide .photo_list--wrap{height:100%}
#op_layout #op_slide .photo_list--slider{height:100%;}
#op_layout #op_slide .slick-list{height:100%;}
#op_layout #op_slide .photo_list--item{text-align:center;}
#op_layout #op_slide .photo_list--item a{display: inline-block;position: relative;width: auto;height:100%;/* padding-left: 210px; */}
#op_layout #op_slide .photo_list--item .icon{display: block;overflow: hidden;position:absolute;top: 30px;left:0;width: 142px;height: 142px;padding:0;margin: 0;border-radius:50%;}
#op_layout #op_slide .photo_list--item .tit,
#op_layout #op_slide .photo_list--item .txt{ width:100%; display:block;}
#op_layout #op_slide .photo_list--item .tit{display: block;position: relative;margin-top: 15px;font-size:22px;color:#fff;line-height:26px;font-family: 'GmarketSans';font-weight: bold;text-align: left;}
#op_layout #op_slide .photo_list--item .tit:before{display:inline-block;position: relative;top: -3px;width:32px;height:30px;margin-right: 7px;background:url(../images/main/op_il.png);content:'';vertical-align:middle;}
#op_layout #op_slide .photo_list--item .txt{width: 370px;height: 84px;background: url(../images/main/op_txt.png);font-size: 0;}
#op_layout #op_slide .slick-counter{display: inline-block;position: absolute;right: 121px;top: 90px;text-align:right;color: #fff;}
#op_layout #op_slide .slick-counter span{display:inline-block;margin: 0 5px;font-size: 18px;font-weight: bold;color:#fff;}
#op_layout #op_slide .slick-counter .current{text-decoration:underline;}
#op_layout #op_slide .slick-counter .current:before{}
#op_layout #op_slide .slick-counter .total{position:relative;}
#op_layout #op_slide .slider-control{position:absolute;left:0;top: 42%;width:100%;padding: 0 40px;}
#op_layout #op_slide .slick-arrow{display: inline-block;float: left;width: 46px;height: 46px;margin-left: auto;border: 1px solid #fff;border-width: 0px 2px 2px 0;font-size:0;}{display:inline-block;position:absolute;top: 41%;z-index:5;width: 38px;height: 65px;font-size:0;text-indent:-9999px;padding:0;margin:-20px 0 0 0;}
#op_layout #op_slide .prev{transform: rotate(135deg);}
#op_layout #op_slide .next{float: right;right:30px;transform: rotate(-45deg);}
#op_layout #op_slide .pause,
#op_layout #op_slide .play{position:absolute;top: 29%;right: 95px;width:17px;height:17px;border:1px solid #fff;}
#op_layout #op_slide button.pause,
#op_layout #op_slide button.play{}
#op_layout #op_slide button.pause:before,
#op_layout #op_slide button.pause:after{content:'';display:block;position:absolute;background: #fff;width: 2px;height: 7px;top: 50%;transform: translateY(-50%);}
#op_layout #op_slide button.pause:before{left: 31%;}
#op_layout #op_slide button.pause:after{right: 31%;}
#op_layout #op_slide button.play:before{content:'';display:block;position:absolute;top:50%;left:50%;border-top: 3.5px solid transparent;border-left: 5px solid #fff;border-bottom: 3.5px solid transparent;transform: translate(-50%, -50%);}
#op_layout .opBtn_close{display:none;}
#op_layout .opBtn_open{ display:inline-block;}
#op_layout.on .opBtn_close{ display:inline-block;}
#op_layout .opBtn_close,
#op_layout .opBtn_open{ padding:0; margin:0; position:absolute;}
#op_layout .opBtn_open{bottom: -61px;right: 30px;z-index:1;width: 100px;height: 34px;background-color: #242b38;border-radius: 0 0 10px 10px;font-size: 12px;color:#fff;font-family:'gmarketsans';font-weight:bold;line-height:34px;}
#op_layout .opBtn_open b{color:#ffdf24}
#op_layout.on .opBtn_close{top: 0;right:0;}
#op_layout.on .opBtn_close a{display: inline-block;z-index:1;margin: 17px 30px 0 0;font-size: 16px;color: #fff;vertical-align: top;}
#op_layout.on .opBtn_close a:before{display: inline-block;width: 30px;height: 29px;margin-right: 10px;background: url(../images/main/op_close.png);vertical-align: middle;content:"";}
#op_layout.on .opBtn_close .today_close{margin: 20px 30px 10px 10px;display:inline-block;}
#op_layout.on .opBtn_close .today_close input{ width:16px; height:16px; background-color:#fff; padding:0; margin:2px; display:inline-block; border:1px solid #000; float:left;}
#op_layout.on .opBtn_close .today_close label{font-size:14px;color:#fff;line-height:20px;padding:0;margin:0;display:inline-block;float:left;}

@media (max-width:1279px){
    #op_layout .opBtn_open{right: 90px;}
    #op_layout.on .opBtn_close a{right:90px;}
    #op_layout .op_wrap{ width:100%;}
}
@media (max-width: 1023px){
    #op_layout.on .opBtn_close .today_close{margin: 22px 20px 0 0;}
    #op_layout .opBtn_open,
    #op_layout.on .opBtn_close a{bottom: -114px;right: 30px;}
    #op_layout .opBtn_open:before,
    #op_layout.on .opBtn_close a:before{width:30px;height:29px;background-size: 100%;}
    #op_layout #op_slide .photo_list--item a{padding-left: 130px;padding-top: 23px;}
    #op_layout #op_slide .photo_list--item .icon{top: 41px;width:95px;height:95px;}
    #op_layout #op_slide .photo_list--item .txt{width: 250px;height: 56px;background-size:100%;}
    #op_layout #op_slide .photo_list--item .tit{font-size:18px;}
    #op_layout #op_slide .slick-counter{bottom: 15px;right: auto;top:auto;left: 50%;margin-left: -33px;}
    #op_layout #op_slide .pause, #op_layout #op_slide .play{left:50%;right:auto;bottom: -50px;top:auto;margin-left: 36px;}

}
@media (max-width:767px){
    #op_layout #op_slide{height:162px;}
    #op_layout .opBtn_open{right: 10px;bottom: -100px;}
    #op_layout.on .opBtn_close a{right:60px;}
    #op_layout .op_wrap .obj .item .box{ padding:0;}
    #op_layout .op_wrap .obj .item .icon,
    #op_layout .op_wrap .obj .item .box:after{ display:none;}
    #op_layout #op_slide .photo_list--item a{padding: 40px 20px 0;}
    #op_layout #op_slide .photo_list--item img{width: auto;height: 80px;}
    #op_layout #op_slide .photo_list--item .icon{top: 54px;width:61px;height:61px;}
    #op_layout #op_slide .photo_list--item .tit{margin-top: 7px;font-size:12px;}
    #op_layout #op_slide .photo_list--item .tit:before{width:20px;height:20px;background-size:100%;}
    #op_layout #op_slide .photo_list--item .txt{height:36px;width: 161px;}
    #op_layout #op_slide .slick-counter span{font-size:15px;}
    #op_layout #op_slide .slick-arrow{width: 29px;height: 29px;border-width: 0px 1px 1px 0;}
    #op_layout #op_slide .slider-control{padding: 0 10px;}
    #op_layout .opBtn_open:before, #op_layout.on .opBtn_close a:before{width:18px;height:18px;}
    #op_layout #op_slide .pause, #op_layout #op_slide .play{bottom: -47px;margin-left: 16px;}

}



